{
  "name": "with-typescript-graphql",
  "version": "0.1.0",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"next\" \"yarn generate:client:watch\" \"yarn generate:nexus:watch\" \"yarn generate:prisma:watch\"",
    "build": "next build",
    "test": "jest",
    "start": "next start",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:client",
    "generate:client": "graphql-codegen --config codegen.yml",
    "generate:client:watch": "graphql-codegen --config codegen.yml --w",
    "generate:prisma": "prisma generate",
    "generate:prisma:watch": "prisma generate --watch",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/graphql",
    "generate:nexus:watch": "ts-node-dev --respawn --transpile-only -P nexus.tsconfig.json src/pages/api/graphql"
  },
  "dependencies": {
    "@chakra-ui/core": "next",
    "@headlessui/react": "^0.2.0",
    "@nexus/schema": "^0.17.0",
    "@paljs/plugins": "^2.5.7",
    "@prisma/client": "^2.10.2",
    "apollo-server-micro": "^2.16.1",
    "bcrypt": "^5.0.0",
    "clsx": "^1.1.1",
    "cookies": "^0.8.0",
    "framer-motion": "latest",
    "graphql": "15.4.0",
    "graphql-request": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "next": "10.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.10.1",
    "react-icons": "^3.11.0",
    "react-query": "^2.26.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@graphql-codegen/cli": "1.19.1",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-graphql-request": "^2.0.2",
    "@graphql-codegen/typescript-operations": "1.17.9",
    "@prisma/cli": "^2.10.2",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^10.0.1",
    "babel-jest": "26.6.3",
    "concurrently": "^5.3.0",
    "jest": "26.6.3",
    "postcss-import": "^13.0.0",
    "prettier": "^2.1.2",
    "tailwindcss": "^1.9.6",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
